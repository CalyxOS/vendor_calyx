<?xml version="1.0" encoding="utf-8"?>
<user-types version='0'>
    <!-- see https://source.android.com/docs/devices/admin/custom-user-types -->
    <profile-type
        name='android.os.usertype.profile.MANAGED'
        max-allowed-per-parent='3'
        icon-badge='?android:drawable/ic_corp_icon_badge_case'
        badge-plain='?android:drawable/ic_corp_badge_case'
        badge-no-background='?android:drawable/ic_corp_badge_no_background' >
        <badge-labels>
            <item res='?android:string/managed_profile_label_badge' />
            <item res='?android:string/managed_profile_label_badge_2' />
            <item res='?android:string/managed_profile_label_badge_3' />
        </badge-labels>
        <badge-colors>
            <item res='?android:color/profile_badge_1' />
            <item res='?android:color/profile_badge_2' />
            <item res='?android:color/profile_badge_3' />
        </badge-colors>
        <badge-colors-dark>
            <item res='?android:color/profile_badge_1_dark' />
            <item res='?android:color/profile_badge_2_dark' />
            <item res='?android:color/profile_badge_3_dark' />
        </badge-colors-dark>
    </profile-type>
</user-types>
